{
  "compilerOptions": {
    "target": "ES6",
    "module": "ESNext",
    "moduleResolution": "Node",
    "strict": true,
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "declaration": true,
    "outDir": "dist",
    "skipLibCheck": true,
    "composite": true,
    "jsxImportSource": "react",
    "types": ["@react-three/fiber"]
  },
  "include": ["src/**/*"]
}
